{
  "name": "typekit-i18n",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "gen": "pnpm -w run gen:legacy",
    "build": "vite build && tsc -p tsconfig.build.json",
    "dev": "vite build --watch",
    "lint": "pnpm -w exec eslint \"src/**/*.ts\"",
    "test": "pnpm -w exec vitest run --passWithNoTests",
    "typecheck": "pnpm -w exec tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^6.2.0"
  }
}
