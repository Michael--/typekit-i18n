{
  "name": "@typekit-i18n/playground-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist generated",
    "dev": "vite",
    "gen": "node --import tsx scripts/gen.ts",
    "build": "pnpm run gen && vite build",
    "lint": "eslint \"src/**/*.ts\"",
