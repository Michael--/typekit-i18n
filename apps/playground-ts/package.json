    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "typekit-i18n": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^6.2.0"
  }
}
